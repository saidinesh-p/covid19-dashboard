pipeline {
    agent any
    tools { 
        maven 'Apache Maven 3.6.0' 
        jdk 'java' 
    }
    stages {
        stage ('Initialize') {
            steps {
                sh 'mvn clean'
            }
        }

        stage ('Build') {
            steps {
                 sh 'mvn install'
            }
        }
    }
}
